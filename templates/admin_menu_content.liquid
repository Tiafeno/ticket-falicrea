<table id="tickets">
    <tr>
        <th>Code de sécurité</th>
        <th>Client</th>
        <th>Commande</th>
        <th>Date</th>
    </tr>

    {% for ticket in tickets %}
        <tr>
            <td>{{ ticket.code }}</td>
            <td><b>{{ ticket.author_email }}</b> « {{ ticket.author_name }} »</td>
            <td><a href="{{ ticket.order_url }}">Order #{{ ticket.order_id }}</a></td>
            <td>{{ ticket.date }}</td>
        </tr>
    {% endfor %}


</table>